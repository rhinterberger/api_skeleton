{
  "name": "API-Skeleton",
  "version": "0.0.1",
  "description": "Backend Api Skeleton",
  "type": "module",
  "main": "src/application_main.js",
  "scripts": {
    "start": "node --experimental-modules  --experimental-json-modules",
    "devstart": "nodemon --experimental-modules  --experimental-json-modules --trace-uncaught"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rhinterberger/api_skeleton.git"
  },
  "author": "Rainer Hinterberger",
  "license": "MIT",
  "dependencies": {
    "cors": "latest",
    "dotenv": "latest",
    "express": "latest",
    "express-jwt": "latest",
    "express-jwt-permissions": "latest",
    "express-promise-router": "latest",
    "express-session": "latest",
    "helmet": "latest",
    "pg": "latest",
    "winston": "latest",
    "authmodule": "file:./src/modules/userauth",
    "testmodule": "file:./src/modules/testmodule"
  },
  "devDependencies": {
    "nodemon": "latest"
  }
}
